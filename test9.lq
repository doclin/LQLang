double[6] R;


int main()
{
    R[1] = -0.99;
    R[2] = -1.0;
    R[3] = 5;
    R[4] = 4.01;
    R[5] = 3.0;
    int i, j, swap;
    i = 6;
    while (i != 1){
        swap = 0;
        j = 1;
        while (j < i-1){
            if ( R[j+1] < R[j]){
                swap = 1;
                R[0] = R[j];
                R[j] = R[j+1];
                R[j+1] = R[0];
            }
            j = j + 1;
        }
        i = i - 1;
        if(swap != 1){
            i = 1;
        }
    }
    int k;
    k = 1;
}